{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "plugins": [
        "@typescript-eslint"
    ],
    "overrides": [
        {
            "files": ["*.{ts,tsx}"],
            "rules": {
                "require-jsdoc": "off"
            },
            "parser": "@typescript-eslint/parser",
            "parserOptions": {
                "ecmaVersion": "latest",
                "sourceType": "module"
            }
        }
    ]
}
