{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "plugins": [
        "@typescript-eslint"
    ],
    "ignorePatterns": ["**/*.js", "dist"],
    "rules": {
        "require-jsdoc": "off"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    }
}
